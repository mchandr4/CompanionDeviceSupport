  <string name="settings_entry_title">دستگاه همراه</string>
  <string name="default_device_name">دستگاه مرتبط</string>
  <string name="add_device_title">افزودن دستگاه مرتبط</string>
  <string name="add_associated_device_title">اتصال دستگاه همراه</string>
  <string name="add_associated_device_instruction">برای کمک به مدیریت تجربه رانندگی‌تان، می‌توانید از تلفن خود به‌عنوان دستگاه همراه استفاده کنید</string>
  <string name="associated_device_install_app">مطمئن شوید <b>برنامه همراه</b> را در تلفنتان نصب کرده‌اید</string>
  <string name="associated_device_pairing_code_title">تأیید کد در تلفن</string>
  <string name="associated_device_pairing_message">تأیید کنید این کد با کدی است که در تلفنتان نشان داده شده است مطابقت دارد</string>
  <string name="associated_device_success">دستگاه باموفقیت مرتبط شد</string>
  <string name="associated_device_select_device">برنامه را باز کنید و به &lt;b&gt;%1$s&lt;/b&gt; متصل شوید</string>
  <string name="associated_device_open_app">برنامه را باز کنید و دستورالعمل‌های درون آن را دنبال کنید</string>
  <string name="remove_associated_device_title">&lt;b&gt;%1$s&lt;/b&gt; فراموش شود؟</string>
  <string name="remove_associated_device_message">این خودرو دیگر با این دستگاه مرتبط نخواهد بود. به‌علاوه، باید این خودرو را از برنامه همراه بردارید.</string>
  <string name="device_removed_success_toast_text">%1$s فراموش شد</string>
  <string name="device_removed_failure_toast_text">%1$s فراموش نشد</string>
  <string name="continue_setup_toast_text">ادامه راه‌اندازی در تلفن</string>
  <string name="error_screen_title">مشکلی پیش آمد</string>
  <string name="error_screen_message">متأسفانه، این صفحه‌نمایش به‌درستی نشان داده نمی‌شود. لطفاً دوباره امتحان کنید.</string>
  <string name="turn_on_bluetooth_title">روشن کردن بلوتوث</string>
  <string name="turn_on_bluetooth_message">برای تنظیم تلفن به‌عنوان دستگاه همراه، خودرویتان به بلوتوث نیاز دارد.</string>
  <string name="turn_on_bluetooth_dialog_title">بلوتوث روشن شود؟</string>
  <string name="turn_on_bluetooth_dialog_message">برای وصل شدن به دستگاه همراه، خودرویتان به بلوتوث نیاز دارد.</string>
